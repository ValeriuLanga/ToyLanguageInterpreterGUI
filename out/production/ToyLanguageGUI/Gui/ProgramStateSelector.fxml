<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.ProgramStateSelectorController">
   <children>
      <VBox alignment="CENTER" layoutX="1.0" layoutY="-2.0" prefHeight="406.0" prefWidth="600.0">
         <effect>
            <Blend />
         </effect>
         <children>
            <Label alignment="CENTER" prefHeight="74.0" prefWidth="600.0" text="Select a program from below to run" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <ListView fx:id="listViewProgramState" prefHeight="250.0" prefWidth="600.0" />
            <Button fx:id="buttonSelect" mnemonicParsing="false" prefHeight="81.0" prefWidth="255.0" text="Select Program">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
